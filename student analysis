import numpy as np
import pandas as pd
import matplotlib.pyplot as plt
import streamlit as st

st.set_page_config(page_title="Placement", layout="wide")

with st.container():
  st.title("Student Report Alalyses")
  st.write("The sun slowly dipped behind the quiet hills, painting the sky in shades of orange and purple. A cool breeze whispered through the trees, carrying the scent of fresh earth. Birds returned to their nests, and silence grew, leaving only the calm rhythm of natureâ€™s heartbeat echoing across the valley.")


with st.container():
  col1, spacer, col2 = st.columns([3,1,3])

  col1.header("Enter Student datails")
  name  =col1.text_input("Enter the name of the student")
  roll = col1.number_input("Enter Roll_no",min_value = 1)
  col1.number_input("Attencence",min_value = 0)


  col2.image("data:image/jpeg;base64,")
  col2.subheader("Select your subject")
  subject = col2.checkbox("Math")
  subject = col2.checkbox("Science")
  subject = col2.checkbox("English")


with st.container():
  st.header("Upload student file")
  col1, spacer, col2 = st.columns([3,1,3])
  upload_file = st.file_uploader("",type = ["csv"])

  if upload_file is not None:
    df = pd.read_csv(upload_file)
    
    if roll and name:
      st.subheader(f"Report of the {name} sir/madam")
      st.dataframe(df)
      marks = df["marks"]
      avg = np.mean(marks)
      max = np.max(marks)
      min = np.min(marks)
      standard = np.std(marks)
      col1.bar_chart(marks)
      col2.title(f"{name} avg marks {avg}")
    else:
      st.header("you enter wrong detail")

with st.container():
